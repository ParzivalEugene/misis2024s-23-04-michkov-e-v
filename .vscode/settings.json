{
  "discord.lowerDetailsEditing": "Learning OOP with Polevoi",
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortAttrs": "explicit"
  },
  "files.exclude": {
    "**/bin": true,
    "**/build": true,
    "**/.venv/": true,
    "**/__pycache__": true
  },
  "files.associations": {
    "iostream": "cpp",
    "numeric": "cpp"
  }
}
